{
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "description": "CreativeCommons.org Local Dev WordPress site via Composer",
  "license": "GPL-2.0-or-later",
  "name": "creativecommons/creativecommons-org-local-dev",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
